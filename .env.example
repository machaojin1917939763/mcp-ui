# API密钥 (OpenAI)
VITE_API_KEY=your_openai_api_key_here

# MCP服务端URL (可选，如不设置将使用模拟工具)
VITE_MCP_SERVER_URL=

# MCP后端服务端口
MCP_SERVER_PORT=3001

# 模型提供商 (openai 或 anthropic)
VITE_MODEL_PROVIDER=openai

# OpenAI模型 (如不设置将使用默认值)
VITE_OPENAI_MODEL=gpt-3.5-turbo-0125

# OpenAI API基础URL (可选，用于自定义API端点或代理)
VITE_OPENAI_BASE_URL=https://api.openai.com/v1 